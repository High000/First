{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"es5",
			"es6",
			"dom"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": false,
		"inlineSourceMap": true,
		"declaration": false,
		"removeComments": false,
		"noImplicitAny": false,
		"outDir": "lib",
		"rootDir": ".",
		"types": [
			"jasmine",
			"node"
		]
	},
	"exclude": [
		"node_modules"
	]
}